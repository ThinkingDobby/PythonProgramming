   "source": [
    "class Calculator:\r\n",
    "    def __init__(self): # 파이썬은 생성자 하나만 작성 가능 - * 이용해 매개변수 다르게 처리 가능\r\n",
    "        self.result = 0\r\n",
    "    \r\n",
    "    def add(self, num):\r\n",
    "        self.result += num\r\n",
    "        return self.result\r\n",
    "\r\n",
    "cal = Calculator()\r\n",
    "print(cal.result)\r\n",
    "print(cal.add(6))"
   ],
