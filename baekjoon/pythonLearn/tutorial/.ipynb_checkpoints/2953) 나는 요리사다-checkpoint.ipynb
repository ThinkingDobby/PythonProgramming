    "m = 0\n",
    "mi = -1\n",
    "for i in range(1, 6):\n",
    "    s = sum(list(map(int, input().split(\" \"))))\n",
    "    if s > m:\n",
    "        m = s\n",
    "        mi = i\n",
    "\n",
    "print(\"%d %d\" % (mi, m))"
