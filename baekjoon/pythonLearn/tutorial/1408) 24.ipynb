   "source": [
    "a = list(map(int, input().split(\":\")))\r\n",
    "b = list(map(int, input().split(\":\")))\r\n",
    "tmp = (b[0] * 3600 + b[1] * 60 + b[2]) - (a[0] * 3600 + a[1] * 60 + a[2])\r\n",
    "if tmp < 0: tmp += 24 * 3600\r\n",
    "print(\"%02d:%02d:%02d\" % (tmp // 3600, (tmp % 3600) // 60, tmp % 60))"
   ],
