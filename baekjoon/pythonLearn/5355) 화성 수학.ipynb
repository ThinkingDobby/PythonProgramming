   "source": [
    "for _ in range(int(input())):\r\n",
    "    n = input().split()\r\n",
    "    tmp = float(n[0])\r\n",
    "    for i in range(1, len(n)):\r\n",
    "        if n[i] == \"@\": tmp *= 3\r\n",
    "        elif n[i] == \"%\": tmp += 5\r\n",
    "        else: tmp -= 7\r\n",
    "    print(\"%0.2f\" %tmp)"
   ],
